{
  "expo": {
    "name": "Target",
    "slug": "target",
    "scheme": "target",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3d44cd"
    },
    "ios": {
      "bundleIdentifier": "com.pabloxt14.target",
      "icon": "./assets/ios-icon.png"
    },
    "android": {
      "package": "com.pabloxt14.target",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3d44cd"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-sqlite"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0109f5ee-3bc2-4119-86bc-2c5ae5709168"
      }
    }
  }
}
